.do_register,.term{ padding-left:82px;}
.term span{ display:inline-block; background:url(../images/tip_ico.gif) no-repeat 0 -182px; padding-left:15px; }
.term span a{ color:#12ADFF; font-size:14px; text-decoration:none;}
.term span a:hover{ color:#12ADFF; font-size:14px; text-decoration:none;}
.btn_do_register{ background:url(../images/user_btn.gif) no-repeat 0px 0px; width:135px; border:none; height:37px; text-decoration:none;  cursor:pointer; display:inline-block; }
.btn_do_register_hover{ background:url(../images/user_btn.gif) no-repeat 0px -37px; width:135px; border:none; height:37px; text-decoration:none;  cursor:pointer; display:inline-block; }
.btn_do_register_active{ background:url(../images/user_btn.gif) no-repeat 0px -74px; width:135px; border:none; height:37px; text-decoration:none;  cursor:pointer; display:inline-block; }
.btn_user_register{ background:url(../images/user_btn.gif) no-repeat 0px 0px; width:135px; border:none; height:37px; text-decoration:none;  cursor:pointer; display:inline-block; }
.btn_user_register_hover{ background:url(../images/user_btn.gif) no-repeat 0px -37px; width:135px; border:none; height:37px; text-decoration:none;  cursor:pointer; display:inline-block; }
.btn_user_register_active{ background:url(../images/user_btn.gif) no-repeat 0px -74px; width:135px; border:none; height:37px; text-decoration:none;  cursor:pointer; display:inline-block; }

.tip_box{ float:left;}
.form_error{ background:url(../images/tip_ico.gif) no-repeat 0 -1px; height:25px; font-size:12px; line-height:25px; padding-left:30px; float:left; display:inline-block; margin-left:10px; color:#787878;}
.form_success{ background:url(../images/tip_ico.gif) no-repeat 0 -50px; height:25px;  font-size:12px; line-height:25px; padding-left:30px; float:left; display:inline-block; margin-left:10px; color:#787878;}
.form_tip{ background:url(../images/tip_ico.gif) no-repeat 0 -108px; height:25px; line-height:25px;  font-size:12px; padding-left:30px; float:right; display:inline-block; margin-left:10px; color:#787878;}
.form_loading{ background:url(../images/ajax_loading.gif) no-repeat -130px -47px; width:25px; height:28px; float:left; display:inline-block; margin-left:10px;}
.btn_go_login{ background:url(../images/go_login.gif) no-repeat; width:113px; height:39px; text-decoration:none; display:block;}