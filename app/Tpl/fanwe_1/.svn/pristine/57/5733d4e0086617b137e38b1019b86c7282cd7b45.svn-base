.index_images { overflow:hidden; width:960px; position:relative; height:320px; }.index_images .roll_box a{ float:left; display:block; margin:0px; padding:0px; width:960px; height:280px;}.index_images .roll_box{ position:absolute; left:0px; top:0px;}.index_images .slider_page{ text-align:center; margin-top:285px;}.index_images .slider_page ul{ display:inline-block;}.index_images .slider_page ul li{ float:left; width:10px; height:20px; background:url(http://t1.fanwe.net:93/zc/app/Tpl/fanwe/images/slider_ico.gif) no-repeat 0px 0px; cursor:pointer; display:inline-block; margin-left:8px; }.index_images .slider_page ul li.current{ float:left; width:10px; height:20px; background:url(http://t1.fanwe.net:93/zc/app/Tpl/fanwe/images/slider_ico.gif) no-repeat 0px -27px; cursor:pointer; display:inline-block; margin-left:8px; }


/*ui*/
.ui-deals-filter{
font-size: 13px;
color: #666;
background: #f8f8f8;
border: #e9e9e9 1px solid;
border-bottom: #d7d7d7 1px solid;
border-radius: 5px;}
.ui-deals-filter-item{ padding:5px 8px 5px 10px; line-height:24px; border-bottom:1px solid #E6E6E6}
.ui-deals-filter-l{ line-height:36px;}
.ui-deals-filter-r{ width: 1130px;}
.ui-deals-filter-r ul{ clear:both;}
.ui-deals-filter-r ul li{ float:left;}
.ui-deals-filter-r ul li a{display: block;
font-weight: normal;
float: left;
height: 24px;
line-height: 24px;
margin: 6px 12px 0px 0px;
padding: 0px 5px 0px 5px;
border-radius: 3px;
_display: inline;
border:1px solid #f8f8f8;
white-space: nowrap; text-decoration:none;}

.ui-deals-filter-r ul li a:hover{color: #fff;
background: #bbb;
border: #a8a8a8 1px solid;}

.ui-deals-filter-r ul li.current a,.ui-deals-filter-r ul li.current a:hover{background: #93c215;
border: #84ad14 1px solid;
color: #fff;
cursor: text;}

.ui-button{ float:left; background:url(../images/ui-buttonbg.gif) no-repeat; height:32px; padding-left:5px;}
.ui-button-not-backgrount{ float:left;height:32px; padding-left:5px;}
.ui-button div{ float:left; background:url(../images/ui-buttonbg.gif) no-repeat; height:32px; }
.ui-button span{ background:none; white-space:nowrap; border:none; padding:0px 15px 0px 10px; height:30px;  font-size:14px; float:left;  line-height:30px; display:inline-block; cursor:pointer;}


.green{ background-position:0px -490px; }
.green div{ background-position:right -140px; }
.green span,.green_hover span,.green_active span{ color:#fff;}

.green_hover{ background-position:0px -525px; }
.green_hover div{ background-position:right -175px; }
.green_active{ background-position:0px -560px; }
.green_active div{ background-position:right -210px; }


.btn_do_login{ background:url(../images/user_btn.gif) no-repeat -148px 0px; width:135px; border:none; height:37px; text-decoration:none;  cursor:pointer; display:inline-block; }
.btn_do_login_hover{ background:url(../images/user_btn.gif) no-repeat -148px -37px; width:135px; border:none; height:37px; text-decoration:none;  cursor:pointer; display:inline-block; }
.btn_do_login_active{ background:url(../images/user_btn.gif) no-repeat -148px -74px; width:135px; border:none; height:37px; text-decoration:none;  cursor:pointer; display:inline-block; }
