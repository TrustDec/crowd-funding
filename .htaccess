<IfModule mod_rewrite.c>
RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f

RewriteRule ^article_cate$  index.php\?ctl=article_cate [QSA,L]
RewriteRule ^(gywm|xsbz|hdbm|mtbd|wyxm|zdsm|gqzc|gonggao)$  index.php\?ctl=article_cate&bs=$1 [QSA,L]
RewriteRule ^(gywm|xsbz|hdbm|mtbd|wyxm|zdsm|gqzc|gonggao)/([^\.&]*)\.html$  index.php\?ctl=article&id=$2 [QSA,L]

RewriteRule ^(forPayLayer)\.html$  forPayLayer.html [L]
RewriteRule ^(dy)\.html$  dy.html [L]

RewriteRule ^([^\.&]*)\.html$  index.php\?rewrite_param=$1 [QSA,L]



</IfModule>